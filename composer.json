{
	"name": "orchestra/control",
	"description": "Control Extension for Orchestra Platform",
	"license": "MIT",
	"keywords": ["orchestra-platform", "orchestral", "extension", "laravel", "rbac", "timezone", "theme"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"classmap": [
			"resources/database/migrations"
		],
		"psr-4": {
			"Orchestra\\Control\\" : "src/"
		}
	},
	"require": {
		"php": ">=5.4.0",
		"orchestra/contracts": "3.1.*"
	},
	"require-dev": {
		"phpspec/phpspec": "~2.1",
		"orchestra/foundation": "3.1.*"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.1-dev"
		}
	},
	"minimum-stability": "dev"
}
