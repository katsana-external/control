{
    "name": "orchestra/control",
    "description": "Control Extension for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "extension", "laravel", "rbac", "theme"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Control\\" : "src/"
        },
        "classmap": [
            "database/migrations"
        ]
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "orchestra/contracts": "^6.0",
        "orchestra/extension": "^6.0",
        "orchestra/support": "^6.0"
    },
    "require-dev": {
        "orchestra/foundation": "^6.0",
        "orchestra/testing": "^6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    },
    "minimum-stability": "dev"
}
