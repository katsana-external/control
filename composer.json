{
    "name": "orchestra/control",
    "description": "Control Extension for Orchestra Platform",
    "keywords": [
        "orchestra-platform",
        "orchestral",
        "extension",
        "laravel",
        "rbac",
        "theme"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Control\\": "src/"
        },
        "classmap": [
            "database/migrations"
        ]
    },
    "require": {
        "php": " ^8.0",
        "illuminate/database": "^9.0",
        "orchestra/contracts": "7.x-dev",
        "orchestra/extension": "7.x-dev",
        "orchestra/support": "7.x-dev"
    },
    "require-dev": {
        "orchestra/foundation": "7.x-dev",
        "orchestra/testing": "7.x-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/extension"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/foundation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/testing"
        }
    ]
}
