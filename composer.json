{
	"name": "orchestra/control",
	"description": "Control Extension for Orchestra Platform 2",
	"license": "MIT",
	"keywords": ["orchestra-platform", "orchestral", "extension", "laravel", "rbac", "timezone", "theme"],
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"classmap": [
			"src/migrations"
		],
		"psr-4": {
			"Orchestra\\Control\\" : "src/Control"
		}
	},
	"require": {
		"php": ">=5.4.0"
	},
	"require-dev": {
		"mockery/mockery": "0.9.*",
		"orchestra/support": "2.2.*",
		"orchestra/testbench": "2.2.*",
		"orchestra/foundation": "2.2.*"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.3-dev"
		}
	},
	"minimum-stability": "dev"
}
